    "@nomiclabs/buidler-waffle": "1.3.4",
    "buidler-gas-reporter": "0.1.3",
    "chai": "4.2.0",
    "coveralls": "3.1.0",
    "dotenv": "8.2.0",
    "ethereum-waffle": "2.5.1",
    "ethers": "4.0.47",
    "prettier": "2.0.3",
