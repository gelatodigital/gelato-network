{
  "name": "gelato-v1",
  "version": "0.0.1",
  "description": "The automation protocol for Ethereum",
  "repository": "https://github.com/gelatodigital/gelato.git",
  "author": "The guys from gelato-digital",
  "private": false,
  "scripts": {
    "coverage": "npx buidler coverage --network coverage",
    "test": "npx buidler test",
    "test-debug": "export BUIDLER_DEBUG=true && yarn test",
    "test-gas": "export REPORT_GAS=true && npx buidler test --network localhost",
    "test-gas-debug": "export BUIDLER_DEBUG=true && npx buidler bre-config --log && yarn test-gas"
  },
  "devDependencies": {
    "@babel/core": "7.7.4",
    "@babel/preset-env": "7.7.4",
    "@babel/register": "7.7.4",
    "@codechecks/client": "^0.1.10",
    "@nomiclabs/buidler": "1.3.3-rc.0",
    "@nomiclabs/buidler-ethers": "1.3.3-rc.0",
    "@nomiclabs/buidler-waffle": "1.3.3-rc.0",
    "buidler-gas-reporter": "^0.1.3",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "2.4.1",
    "ethers": "4.0.47",
    "prettier": "^2.0.3",
    "solc": "^0.6.6",
    "solidity-coverage": "0.7.5",
    "solidity-docgen": "^0.4.1"
  }
