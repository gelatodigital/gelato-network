{
  "name": "@gnosis.pm/dx-services",
  "version": "2.3.6",
  "description": "",
  "main": "src/runPublicApi.js",
  "directories": {
    "test": "test"
  },
  "bin": {
    "dutchx-kovan": "./src/cli/cli-kovan.js",
    "dutchx-rinkeby": "./src/cli/cli-rinkeby.js",
    "dutchx-mainnet": "./src/cli/cli-mainnet.js"
  },
  "scripts": {
    "executornode:ropsten": "export ROPSTEN=ROPSTEN; npx truffle exec executornode.js --network ropsten",
    "console:dev": "npx truffle console --network development",
    "console:rinkeby": "export RINKEBY=RINKEBY; npx truffle console --network rinkeby",
    "console:mainnet": "npx truffle console --network mainnet",
    "migrate:dev": "npx truffle migrate --reset --network development",
    "prep": "mv migrations/*DEV_migrate* .",
    "post": "mv *DEV_migrate* migrations",
    "migrate:rinkeby": "yarn prep; export RINKEBY=RINKEBY; npx truffle migrate --reset --network rinkeby; yarn post",
    "migrate:mainnet": "yarn prep; npx truffle migrate --reset --network mainnet; yarn post",
    "mv-migrations": "mv migrations/* .",
    "mv-migrations-back": "mv *_migrate_* migrations/",
    "int-test-exec": "yarn mv-migrations; truffle test test/integration_tests/onlyExecuteIntTest.js; yarn mv-migrations-back",
    "int-test-withdraw-exec": "yarn mv-migrations; truffle test test/integration_tests/execManualWithdrawIntTest.js; yarn mv-migrations-back",
    "int-test-cancel": "yarn mv-migrations; truffle test test/integration_tests/cancelOrdersIntTest.js; yarn mv-migrations-back",
    "event-values-mint": "truffle exec rinkeby-test-kit/getLogNewExecutionClaimMinted.js",
    "install": "cd $INIT_CWD && npm explore truffle -- npm install solc@0.5.2",
    "start": "npm run api",
    "api": "node src/runPublicApi.js",
    "dev": "DEBUG=*-dx-service:*,*AuctionRepoImpl nodemon src/runPublicApi.js",
    "trace": "DEBUG=* nodemon src/runPublicApi.js",
    "api-rinkeby": "ENV_PATH=env/rinkeby nodemon src/runPublicApi.js",
    "api-kovan": "ENV_PATH=env/kovan nodemon src/runPublicApi.js",
    "api-mainnet": "ENV_PATH=env/mainnet nodemon src/runPublicApi.js",
    "bots-script": "./src/run-bots.sh",
    "bots": "node src/runBots.js",
    "bots-dev": "DEBUG=ERROR-*,WARN-*,INFO-*,DEBUG-* nodemon src/runBots.js",
    "bots-trace": "DEBUG=* node src/runBots.js",
    "bots-rinkeby": "ENV_PATH=env/rinkeby-bots node src/runBots.js",
    "bots-rinkeby-dev": "DEBUG=ERROR-*,WARN-*,INFO-*,DEBUG-* ENV_PATH=env/rinkeby-bots node src/runBots.js",
    "bots-kovan": "ENV_PATH=env/kovan-bots node src/runBots.js",
    "bots-mainnet": "ENV_PATH=env/mainnet-bots node src/runBots.js",
    "test": "NODE_ENV=test jest",
    "test:safe": "NODE_ENV=test jest tests/safe-module --testPathIgnorePatterns=\"node_modules\"",
    "test-coverage": "NODE_ENV=test jest --coverage && cat ./coverage/lcov.info | coveralls",
    "tdd": "NODE_ENV=test jest --watch",
    "test-debug": "NODE_ENV=test DEBUG=* jest",
    "arb-init": "yarn cli deposit 10 WETH && yarn cli deposit 10 UNI && yarn cli add-token-pair --file /tests/data/tokenPairs/local/UNI-WETH.js && yarn cli2 -t 14 && yarn cli state UNI-WETH",
    "arb-ganache": "DEBUG=ERROR-*,WARN-*,INFO-*,DEBUG-* MARKETS=UNI-WETH node src/cli/arb-cli.js",
    "cli": "node src/cli/cli.js",
    "cli-rinkeby": "ENV_PATH=env/rinkeby node src/cli/cli.js",
    "cli-rinkeby-dev": "DEBUG=ERROR-*,WARN-*,INFO-*,DEBUG-* ENV_PATH=env/rinkeby node src/cli/cli.js",
    "cli-kovan": "ENV_PATH=env/kovan src/cli/cli.js",
    "cli-mainnet": "ENV_PATH=env/mainnet node src/cli/cli.js",
    "cli2": "node tests/playground/cli/bot-cli.js",
    "arb": "node src/cli/arb-cli.js",
    "arb-rinkeby": "ENV_PATH=env/rinkeby node src/cli/arb-cli.js",
    "arb-kovan": "ENV_PATH=env/kovan node src/cli/arb-cli.js",
    "arb-mainnet": "ENV_PATH=env/mainnet node src/cli/arb-cli.js",
    "time": "watch -n 5 'npm run cli2 -- --time 0.1'",
    "time-slow": "watch -n 10 'npm run cli2 -- --time 0.1'",
    "time-fast": "watch -n 1 'npm run cli2 -- --time 0.2'",
    "cli2-rinkeby": "ENV_PATH=env/rinkeby node tests/playground/cli/bot-cli.js",
    "cli2-kovan": "ENV_PATH=env/kovan node tests/playground/cli/bot-cli.js",
    "cli2-debug": "DEBUG=* node tests/playground/cli/bot-cli.js",
    "data": "CACHE_ENABLED=false node tests/playground/cli/bot-cli.js --setup",
    "rpc": "ganache-cli -d --mnemonic 'candy maple cake sugar pudding cream honey rich smooth crumble sweet treat' --defaultBalanceEther '100'",
    "truffle": "npx truffle",
    "migrate": "npx truffle migrate",
    "networks": "cd node_modules/@gnosis.pm/dx-contracts && npx truffle network",
    "networks-clean": "cd node_modules/@gnosis.pm/dx-contracts && npx truffle network --clean",
    "contracts-inject-network": "cd node_modules/@gnosis.pm/dx-contracts/ && node scripts/inject_network_info.js",
    "setup": "npm run migrate && npm run data",
    "tasks-claimFunds": "node src/tasks/claimFunds.js",
    "tasks-generateWeeklyReport": "node src/tasks/generateWeeklyReport.js",
    "tasks-generateDailyReport": "node src/tasks/generateDailyReport.js",
    "doc": "esdoc",
    "lint": "eslint .",
    "swagger": "yaml2json src/web/publicApi/static/docs/dx-public-api.yml > src/web/publicApi/static/docs/dx-public-api.json ; yaml2json src/web/bots/static/docs/api/dx-bots-api.yml > src/web/bots/static/docs/api/dx-bots-api.json",
    "format": "prettier-standard 'src/**/*.js'",
    "endow1": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/hil_shell-scripts/endowSeller.sh",
    "create1": "./gelato_tests/gelato_interface_tests/gelato_DX_tests/splitSellAndWithdraw/functional_tests/sh_functional_tests/createSellOrder.sh",
    "skip6": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/hil_shell-scripts/skipSixHours.sh",
    "stage1": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/hil_shell-scripts/stageOne.sh",
    "exec1": "./gelato_tests/gelato_interface_tests/gelato_DX_tests/splitSellAndWithdraw/functional_tests/sh_functional_tests/execSubOrderAndWithdraw.sh",
    "start1": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/hil_shell-scripts/startAuction.sh",
    "close1": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/hil_shell-scripts/closeAuction1.sh",
    "closeX": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/hil_shell-scripts/closeAuction.sh",
    "stage2": "gelato_tests/gelato_interface_tests/gelato_DutchX_tests/hil_shell-scripts/stageTwo.sh",
    "withdraw1": "tuffle exec gelato_tests/gelato_interface_tests/gelato_DX_tests/sh_gelatoDX/withdrawManually.js",
    "close-auction-1": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/hil_shell-scripts/closeAuction1.sh",
    "withdrawExecute": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/gDX_truffle_tests/automated_tests/executeWithdrawTruffelTest.sh",
    "g-listinterface": "truffle exec gelato_tests/gelato_core_tests/g_listInterface.js",
    "gdx-state": "yarn cli state WETH-RDN",
    "gdx-skip": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/gDX_shell-scripts/gDX_skipAhead.sh",
    "gdx-close-auction-1": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/gDX_shell-scripts/gDX_closeAuction1.sh",
    "gdx-start-auction": "gelato_tests/gelato_interface_tests/gelato_DutchX_tests/gDX_shell-scripts/gDX_startAuction.sh",
    "gdx-close-auction": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/gDX_shell-scripts/gDX_closeAuction.sh",
    "gdx-to-first-exec-time": "export SELLER=0xC5fdf4076b8F3A5357c5E395ab970B5B54098Fef; yarn gdx-list-splitsellorder-lazy && yarn gdx-skip6 && yarn gdx-close-auction-1",
    "gdx-to-next-exec-time": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/gDX_shell-scripts/gDX_toNextExecTime.sh",
    "gdx-seller-balance": "SELLER=0xC5fdf4076b8F3A5357c5E395ab970B5B54098Fef; yarn cli balances --account $SELLER",
    "gdx-executor-balance": "EXECUTOR=0x821aEa9a577a9b44299B9c15c88cf3087F3b5544; yarn cli balances --account $EXECUTOR",
    "gdx-endow-seller": "./gelato_tests/gelato_interface_tests/gelato_DutchX_tests/gDX_shell-scripts/gDX_endowSeller.sh",
    "gdx-manual-withdraw": "tuffle exec gelato_tests/gelato_interface_tests/gelato_DutchX_tests/gDX_tests/gDX_functionalTests/gDX_withdrawManually.js",
    "gdx-splitsellorder": "truffle exec gelato_tests/gelato_interface_tests/gelato_DutchX_tests/gDX_tests/gDX_functionalTests/gDX_splitSellOrder.js",
    "gdx-splitsellorder-lazy": "export SELLER=0xC5fdf4076b8F3A5357c5E395ab970B5B54098Fef; yarn gdx-endow-seller && yarn gdx-splitsellorder",
    "gdx-list-splitsellorder-lazy": "export SELLER=0xC5fdf4076b8F3A5357c5E395ab970B5B54098Fef; yarn g-listinterface && yarn gdx-splitsellorder-lazy",
    "gdx-exec": "export EXECUTOR=0x821aEa9a577a9b44299B9c15c88cf3087F3b5544; truffle exec gelato_tests/gelato_core_tests/g_execute.js",
    "gdx-lazy": "yarn setup && yarn gdx-to-first-exec-time && yarn gdx-exec 1",
    "gdx-truffle-mv-deploy": "mv migrations/2_DEV_migrate_dependencies.js migrations/3_deploy_gelato.js .",
    "gdx-truffle-mv-deploy-back": "mv 2_DEV_migrate_dependencies.js 3_deploy_gelato.js migrations",
    "gdx-truffle-list": "yarn gdx-truffle-mv-deploy; truffle test gelato_tests/gelato_interface_tests/gelato_DutchX_tests/gDX_truffle_tests/gDX_truffle_integration_tests/gDX_truffle_list.js; yarn gdx-truffle-mv-deploy-back",
    "gdx-truffle-mint": "yarn gdx-truffle-mv-deploy; truffle test gelato_tests/gelato_interface_tests/gelato_DutchX_tests/gDX_truffle_tests/gDX_truffle_integration_tests/gDX_truffle_mint.js; yarn gdx-truffle-mv-deploy-back",
    "gdx-truffle-exec": "yarn gdx-truffle-mv-deploy; truffle test gelato_tests/gelato_interface_tests/gelato_DutchX_tests/gDX_truffle_tests/gDX_truffle_integration_tests/gDX_truffle_execute.js; yarn gdx-truffle-mv-deploy-back",
    "gdx-truffle-integrationtest": "yarn gdx-truffle-list && yarn gdx-truffle-mint && yarn gdx-close-auction-1 && export ID=1 && yarn gdx-truffle-exec && yarn gdx-state",
    "gdx-truffle-integrationtest-lazy": "yarn setup && yarn gdx-truffle-list && yarn gdx-truffle-mint && yarn gdx-close-auction-1 && export ID=1 && yarn gdx-truffle-exec && yarn gdx-state",
    "oldArchIntegrationTest": "mv ./migrations/2_DEV_migrate_dependencies.js ./2_DEV_migrate_dependencies.js; mv ./migrations/3_deploy_gelato.js ./3_deploy_gelato.js; mv ./migrations/4_deploy_dxGetter.js ./4_deploy_dxGetter.js && truffle test test/integrationTest.js && mv ./2_DEV_migrate_dependencies.js ./migrations/2_DEV_migrate_dependencies.js; mv ./3_deploy_gelato.js ./migrations/3_deploy_gelato.js; mv ./4_deploy_dxGetter.js ./migrations/4_deploy_dxGetter.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gnosis/dx-services.git"
  },
  "license": "MIT",
  "dependencies": {
    "@gnosis.pm/dx-contracts": "2.0.0",
    "@gnosis.pm/dx-price-oracle": "0.2.1",
    "@gnosis.pm/dx-uniswap-arbitrage": "0.1.5",
    "@gnosis.pm/owl-token": "2.0.0",
    "@gnosis.pm/safe-modules": "^0.1.0",
    "@gnosis.pm/util-contracts": "2.0.0",
    "@openzeppelin/contracts": "^2.3.0",
    "@slack/client": "^4.11.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.4",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "git-rev-sync": "^1.12.0",
    "got": "^9.6.0",
    "jest": "^24.8.0",
    "lodash.clonedeep": "^4.5.0",
    "node-cache": "^4.2.0",
    "node-schedule": "^1.3.2",
    "qs": "^6.6.0",
    "raven": "^2.6.4",
    "solc": "0.5.10",
    "truffle": "^5.0.22",
    "truffle-contract": "^3.0.6",
    "truffle-hdwallet-provider": "^1.0.5",
    "web3-provider-engine": "^14.1.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "commander": "^2.20.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "ganache-cli": "^6.4.3",
    "nodemon": "^1.19.1",
    "prettier-standard": "^9.1.1",
    "truffle-assertions": "^0.9.1",
    "yamljs": "^0.3.0"
  }
}
